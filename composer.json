{
    "name": "nedmas/laravel-ulid",
    "description": "This package allows you to easily work with ULIDs (Universally Unique Lexicographically Sortable Identifiers) in your Laravel models.",
    "keywords": [
        "laravel",
        "laravel-ulid",
        "nedmas",
        "ulid"
    ],
    "homepage": "https://github.com/nedmas/laravel-ulid",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tom Densham",
            "email": "nedmas@densham.technology",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "illuminate/database": ">=5.8.0",
        "illuminate/support": ">=5.8.0",
        "robinvdvleuten/ulid": "^5.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "6.7.2",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Nedmas\\Concerns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nedmas\\Concerns\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
